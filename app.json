{
  "name": "AlphaInsider Trading Bot",
  "description": "Follow strategies on AlphaInsider and make trades on your brokerage.",
  "repository": "https://github.com/AlphaInsider/trading_bot",
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "standard-1X",
      "command": "npm start"
    }
  },
  "env": {
    "ALPHAINSIDER_API_KEY": {
      "description": "",
      "required": true
    },
    "STRATEGY_ID": {
      "description": "",
      "required": true
    },
    "ALPACA_KEY": {
      "description": "",
      "required": true
    },
    "ALPACA_SECRET": {
      "description": "",
      "required": true
    },
    "ALPACA_ACCOUNT_TYPE": {
      "description": "",
      "value": "paper",
      "required": false
    }
  }
}